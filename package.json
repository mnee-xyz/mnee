{
  "name": "@mnee/ts-sdk",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.1",
  "description": "Official typescript SDK of MNEE USD stablecoin",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.modern.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.modern.js"
      }
    }
  },
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && microbundle --globals @bsv/sdk=bsv && npm pack",
    "start": "node dist/index.modern.js",
    "test": "cd qa-testing && npm run prepare-package && node run-all.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mnee",
    "bsv",
    "blockchain",
    "stablecoin",
    "1satordinal"
  ],
  "author": "MNEE",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mnee-xyz/mnee.git"
  },
  "homepage": "https://docs.mnee.io",
  "bugs": {
    "url": "https://github.com/mnee-xyz/mnee/issues"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "microbundle": "^0.15.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@bsv/sdk": "^1.3.30",
    "bip39": "^3.1.0"
  },
  "overrides": {
    "chalk": "5.3.0",
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1"
  }
}
