{
  "name": "mnee",
  "version": "2.1.4",
  "description": "A simple package for interacting with the MNEE USD",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/mnee.js",
  "module": "dist/mnee.module.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mnee.module.js",
      "require": "./dist/mnee.js"
    }
  },
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && microbundle --globals @bsv/sdk=bsv && npm pack",
    "build:test": "npm run clean && microbundle --globals @bsv/sdk=bsv",
    "start": "node dist/mnee.modern.js",
    "test": "npm run build:test && node --import tsx test/transaction.test.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mnee",
    "bsv",
    "blockchain"
  ],
  "author": "Dan Wagner",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.14.0",
    "dotenv": "^17.2.0",
    "microbundle": "^0.15.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@bsv/sdk": "^1.3.30",
    "js-1sat-ord": "^0.1.80"
  }
}
